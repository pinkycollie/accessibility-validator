{
  "name": "accessibility-validator",
  "version": "1.0.0",
  "description": "PinkSync Accessibility Validator - Deaf-First Accessibility Automation",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "build:lightweight": "bash scripts/build.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "echo \"No tests configured yet\"",
    "test:api": "pip install pytest pytest-asyncio && pytest api/tests/ -v",
    "setup": "bash scripts/setup-dev.sh",
    "docker:build": "docker build -t accessibility-validator .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "update-deps": "npm update && pip install --upgrade -r requirements.txt"
  },
  "dependencies": {
    "@types/node": "22.5.5",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "next": "^14.2.13",
    "postcss": "^8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.12",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
